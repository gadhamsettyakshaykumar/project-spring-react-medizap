package com.mahesvara.Medizap1.repositories;

import java.util.List;

import org.springframework.data.jpa.repository.JpaRepository;

import com.mahesvara.Medizap1.model.MedicineModel;

public interface MedicineRepository extends JpaRepository<MedicineModel, Integer> {

	List<MedicineModel> findbyName(String name);

}
